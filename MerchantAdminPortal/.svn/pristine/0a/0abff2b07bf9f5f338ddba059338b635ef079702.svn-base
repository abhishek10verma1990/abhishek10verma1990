package com.npst.upi.portal.merchant.dto;

import java.io.Serializable;

public class MerchantStatusDto implements Serializable{
	private static final long serialVersionUID = 1L;
	

	
	
	private String merchantVPA;

	private String merchantAccountNo;
	
	private int requestType;
	
	private String remarks;

	

	public String getMerchantVPA() {
		return merchantVPA;
	}

	public void setMerchantVPA(String merchantVPA) {
		this.merchantVPA = merchantVPA;
	}

	public String getMerchantAccountNo() {
		return merchantAccountNo;
	}

	public void setMerchantAccountNo(String merchantAccountNo) {
		this.merchantAccountNo = merchantAccountNo;
	}

	

	public int getRequestType() {
		return requestType;
	}

	public void setRequestType(int requestType) {
		this.requestType = requestType;
	}

	
	public String getRemarks() {
		return remarks;
	}

	public void setRemarks(String remarks) {
		this.remarks = remarks;
	}

	public MerchantStatusDto() {
		super();
		// TODO Auto-generated constructor stub
	}

	


	

	public MerchantStatusDto(String merchantVPA, String merchantAccountNo, int requestType, String remarks) {
		super();
		this.merchantVPA = merchantVPA;
		this.merchantAccountNo = merchantAccountNo;
		this.requestType = requestType;
		this.remarks = remarks;
	}

	@Override
	public String toString() {
		return "MerchantStatusDto [merchantVPA=" + merchantVPA + ", merchantAccountNo=" + merchantAccountNo
				+ ", requestType=" + requestType + ", remarks=" + remarks + "]";
	}

	
}
