package com.npst.upi.portal.merchant.dto;

import org.springframework.stereotype.Component;

@Component
public class AuthorizerDto {

	private int statuscode;
	private String statusvalue;
	private String merhantname;
	private String merchantVPA;
	private String merchantAccountNo;
	
	public int getStatuscode() {
		return statuscode;
	}
	public void setStatuscode(int statuscode) {
		this.statuscode = statuscode;
	}
	public String getStatusvalue() {
		return statusvalue;
	}
	public void setStatusvalue(String statusvalue) {
		this.statusvalue = statusvalue;
	}
	public String getMerhantname() {
		return merhantname;
	}
	public void setMerhantname(String merhantname) {
		this.merhantname = merhantname;
	}
	public String getMerchantVPA() {
		return merchantVPA;
	}
	public void setMerchantVPA(String merchantVPA) {
		this.merchantVPA = merchantVPA;
	}
	public String getMerchantAccountNo() {
		return merchantAccountNo;
	}
	public void setMerchantAccountNo(String merchantAccountNo) {
		this.merchantAccountNo = merchantAccountNo;
	}
	
	
}
