package com.npst.upi.portal.merchant.utility;

/**
 * @author Rahul Chaudhary
 *
 */
public enum MerchantRegErrorCode {

	DUPLICATE_VPA("MR01", "Duplicate VPA"), 
	ACC_NO_INVALID("MR02", "Account no not valid"), 
	IFSC_CODE_INVALID("MR03", "IFSC code not valid"), 
	ORG_NAME_INVALID("MR04", "Org name not valid"), 
	MCC_CODE_INVALID("MR05", "MCC Code not valid"), 
	PAN_NUMBER_INVALID("MR06", "PAN number not valid"), 
	MOBILE_NO_INVALID("MR07", "Mobile no not valid"), 
	VPA_LENGTH_INVALID("MR08", "VPA length not valid"), 
	VPA_INVALID("MR09", "VPA not valid");
	
	private final String code;
	private final String desc;
	
	MerchantRegErrorCode(String code, String desc) {
		this.code=code;
		this.desc=desc;
	}

	public String getCode() {
		return code;
	}

	public String getDesc() {
		return desc;
	}
	
}
